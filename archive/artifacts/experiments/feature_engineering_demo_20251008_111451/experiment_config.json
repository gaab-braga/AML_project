{
  "experiment_name": "feature_engineering_demo",
  "experiment_type": "feature_engineering",
  "description": "Demonstração do pipeline de feature engineering com experiment manager",
  "data": {
    "source": "HI-Small_Trans.csv",
    "train_samples": 1000,
    "features_count": 15,
    "target_distribution": {
      "0": 937,
      "1": 63
    }
  },
  "feature_engineering": {
    "temporal_features": {
      "rolling_window": 30,
      "aggregation_functions": [
        "sum",
        "mean",
        "std",
        "min",
        "max"
      ],
      "group_by": "Account"
    },
    "categorical_encoding": {
      "method": "target_encoding",
      "columns": [
        "Dest Account",
        "Payment Format",
        "From Bank",
        "Account",
        "To Bank"
      ],
      "smoothing": 0.2
    },
    "outlier_detection": {
      "method": "isolation_forest",
      "contamination": 0.01
    }
  },
  "feature_selection": {
    "rfe": {
      "n_features_to_select": 10,
      "base_model": "LogisticRegression"
    },
    "feature_importance": {
      "model": "RandomForestClassifier",
      "n_estimators": 100
    }
  },
  "artifacts": {
    "target_encoders": [
      "account_encoder",
